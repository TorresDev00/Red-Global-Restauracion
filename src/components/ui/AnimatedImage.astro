---
import { Image } from "astro:assets";
const { url , alt } = Astro.props;
---

<section
    class="h-screen max-md:h-auto md:flex items-center justify-center overflow-hidden my-12 max-md:my-2"
>
    <Image
        src={url}
        alt={alt}
        width={700}
        height={700}
        class="animated-image w-full h-auto"
        loading="lazy"
    />
</section>
