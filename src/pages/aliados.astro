---
import Layout from "../layouts/Layout.astro";
import AnimatedImage from "../components/ui/AnimatedImage.astro";
import FlipCard from "../components/ui/FlipCard.astro";
import { Image } from "astro:assets";
import "swiper/css";
import "swiper/css/pagination";
import 'aos/dist/aos.css';
---

<Layout title="Aliados">
  <style>
    /* Estilos opcionales para personalizar la paginación */
    :root {
      --swiper-pagination-color: #96bd0d; /* Color de los bullets activos */
      --swiper-pagination-bullet-size: 8px; /* Tamaño de los bullets */
      --swiper-pagination-bullet-inactive-color: #999999; /* Color de los bullets inactivos */
      --swiper-pagination-bullet-inactive-opacity: 0.5; /* Opacidad de los bullets inactivos */
    }

    .video-youtube {
      border-radius: 18px;
      overflow: hidden;
    }

    @media (max-width: 767px) {
      .video-youtube {
        border-radius: 0;
      }
    }

    .video-wesley {
      display: block; /* Asegúrate de que sea un bloque para aplicar los estilos correctamente */
      border-radius: 0 0 18px 18px; /* Esquinas inferiores redondeadas */
      overflow: hidden; /* Evita que el contenido salga de los bordes redondeados */
    }

    @media (max-width: 767px) {
      .video-wesley {
        border-radius: 0 0 0 0;
      }
    }

    .img {
      transition: opacity 0.5s ease-in-out; /* Suavizar cambio */
      opacity: 0; /* Ocultar por defecto */
      z-index: 0; /* Asegurarse de que las imágenes no se superpongan incorrectamente */
    }

    .img.visible {
      opacity: 1; /* Imagen visible */
      z-index: 1; /* La imagen visible debe estar al frente */
    }

    .img.hidden {
      opacity: 0; /* Imagen oculta */
      z-index: 0; /* Imagen oculta en el fondo */
    }
  </style>

  <section class="pt-20 pb-5 container mx-auto max-w-7xl">
